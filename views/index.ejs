<%- include("partials/header") -%>

<section>
      <div class="tbl-header">
        <table cellpadding="0" cellspacing="0" border="0">
        <thead>
          <tr>
            <th>Symbol</th>
            <th>Name</th>
            <th>Price</th>
            <th>Change</th>
            <th>Volume</th>
          </tr>
        </thead>
      </table>
    </div>
    <div class="tbl-content">
      <table cellpadding="0" cellspacing="0" border="0">
        <tbody>
          <tr>
  <div id= "table.tbl-content">
    <table>
        <tr>
        <% for (var i = 0; i < symlist.length; i++) { %>
         <tr>
           <td><%= symlist[i] %></td>
           <td><%= namelist[i] %></td>
           <td><%= pricelist[i] %></td>
           <td><%= changelist[i] %></td>
           <td><%= volumelist[i] %></td>
         </tr>    
        <% } %>
     </table>
  </div>
  </tr>
        </tbody>
      </table>
    </div>
  </section>

  <%- include("partials/footer") -%>